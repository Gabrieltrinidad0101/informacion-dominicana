{
  "name": "informacion-dominicana",
  "version": "1.0.0",
  "description": "![Logo](https://github.com/Gabrieltrinidad0101/informacion-dominicana/blob/main/frontend/static/img/logo.png)",
  "main": "main.js",
  "type": "module",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "downloadLinks": "nodemon --legacy-watch ./projects/downloadLinks/main.js --watch projects/downloadLinks",
    "download": "nodemon  --legacy-watch  ./projects/download/main.js --watch ./projects/download",
    "events": "nodemon  --legacy-watch  ./projects/events/main.js --watch ./projects/events",
    "frontend": "npm run start --prefix ./projects/frontend",
    "admin": "npm run start --prefix ./projects/admin",
    "postDownload": "nodemon  --legacy-watch  ./projects/postDownload/main.js --watch ./projects/postDownload",
    "postDownload-test": "vitest run ./projects/postDownload/test",
    "extractedText": "nodemon  --legacy-watch  ./projects/extractedText/main.js --watch ./projects/extractedText",
    "extractedTextAnalyzer": "nodemon  --legacy-watch  ./projects/extractedTextAnalyzer/main.js --watch ./projects/extractedTextAnalyzer",
    "textAnalysisAI": "nodemon  --legacy-watch  ./projects/aiTextAnalyzer/main.js --watch ./projects/aiTextAnalyzer",
    "insertData": "nodemon  --legacy-watch  ./projects/insertData/main.js --watch ./projects/insertData",
    "exportToJson": "nodemon  --legacy-watch  ./projects/exportToJson/main.js --watch ./projects/exportToJson",
    "filesManager": "nodemon  --legacy-watch  ./projects/filesManager/main.js --watch ./projects/filesManager",
    "auth": "nodemon  --legacy-watch  ./projects/auth/main.js --watch ./projects/auth",
    "uploadFiles": "nodemon  --legacy-watch  ./projects/uploadFiles/src/main.js --watch ./projects/uploadFiles",
    "downloadLinks-pro": "node projects/downloadLinks/main.js",
    "download-pro": "node ./projects/download/main.js",
    "events-pro": "node ./projects/events/main.js",
    "postDownload-pro": "node ./projects/postDownload/main.js",
    "extractedText-pro": "node ./projects/extractedText/main.js",
    "extractedTextAnalyzer-pro": "node ./projects/extractedTextAnalyzer/main.js",
    "textAnalysisAI-pro": "node ./projects/aiTextAnalyzer/main.js",
    "insertData-pro": "node ./projects/insertData/main.js",
    "exportToJson-pro": "node ./projects/exportToJson/main.js",
    "filesManager-pro": "node ./projects/filesManager/main.js ",
    "auth-pro": "node ./projects/auth/main.js",
    "uploadFiles-pro": "node ./projects/uploadFiles/src/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.1",
    "nodemon": "^3.1.10"
  },
  "devDependencies": {
    "vitest": "^4.0.16"
  }
}
