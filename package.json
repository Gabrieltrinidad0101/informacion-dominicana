  "main": "index.js",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "downloadLinks": "nodemon projects/downloadLinks/main.js --watch projects/downloadLinks",
    "download": "nodemon projects/download/main.js --watch projects/download",
    "events": "nodemon projects/events/main.js --watch projects/events",
